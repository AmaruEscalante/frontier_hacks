{
  "name": "vibes",
  "version": "0.1.0",
  "description": "Voice-controlled app builder combining Gemini Live and Lovable",
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.42",
    "@anthropic-ai/sdk": "^0.69.0",
    "@google/genai": "^0.14.0",
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "e2b": "^2.7.0",
    "eventemitter3": "^5.0.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.3",
    "react-syntax-highlighter": "^15.6.1",
    "sass": "^1.80.6",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.1"
  },
  "scripts": {
    "start": "PORT=3000 react-scripts start",
    "server": "cd ../claude-code-fastapi && uvicorn app.main:app --reload --port 8000",
    "server:old": "PORT=3001 node --max-http-header-size=160000 server.mjs",
    "dev": "concurrently --names \"BACKEND,REACT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run server\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "typescript": "^5.6.3",
    "untildify": "4.0.0"
  }
