{
    "mcpServers": {
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "Authorization": "Bearer ${CONTEXT7_API_KEY}"
            }
        },
        "sentry": {
            "command": "npx",
            "args": ["-y", "mcp-remote@latest", "https://mcp.sentry.dev/mcp"]
        }
    }
}
