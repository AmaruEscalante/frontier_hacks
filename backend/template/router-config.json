            "models": [
                "moonshotai/kimi-k2-instruct-0905"
            ],
            "transformer": {
                "use": ["groq"],
                "request": {
                    "max_tokens": 8192
                }
            }
        }
    ],
    "Router": {
        "default": "groq,moonshotai/kimi-k2-instruct-0905"
    }
}