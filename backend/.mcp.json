{
    "mcpServers": {
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "Authorization": "Bearer ${CONTEXT7_API_KEY}"
            }
        },
        "sentry": {
            "type": "http",
            "url": "https://mcp.sentry.dev",
            "headers": {
                "Authorization": "Bearer ${SENTRY_AUTH_TOKEN}"
            }
        }
    }
}